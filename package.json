    "ts-jest": "^24.2.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check:ts"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint --fix"
    ],
    "*.{json,css,scss,md,js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "npm t -- --coverage --changedSince=origin/master",
    "prettier:fix": "pretty-quick --staged",
    "prettier": "prettier --write '**/*.{js,jsx,json,css,scss,less,md,ts,tsx}'",
    "preversion": "npm run check:ts && npm run eslint && npm run test:coverage",
    "eslint": "eslint \"**/*.ts\" \"**/*.tsx\"",
    "check:ts": "tsc --noEmit -p tsconfig.json",
    "build": "next build",
    "start": "cross-env NODE_ENV=development API_URL=http://localhost:9000 FE_API_URL=/api nodemon -w server.ts --exec ts-node --project tsconfig.server.json server.ts",
    "fake-api": "dyson .fakeAPI",
    "fake-api:watch": "nodemon --watch .fakeAPI --exec dyson .fakeAPI"
  },
