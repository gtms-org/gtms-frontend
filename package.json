  "husky": {
    "hooks": {
      "commit-msg": "bash scripts/prepareCommit.sh",
      "pre-commit": "lint-staged",
      "pre-push": "npm run preversion"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{json,css,scss,md,js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "npm t -- --coverage --changedSince=origin/master",
    "prettier:fix": "pretty-quick --staged",
    "prettier": "prettier --write '**/*.{js,jsx,json,css,scss,less,md,ts,tsx}'",
    "preversion": "npm run check:ts && npm run eslint && npm run test:coverage",
    "eslint": "eslint \"**/*.ts\" \"**/*.tsx\"",
    "check:ts": "tsc --noEmit -p tsconfig.json",
    "build": "next build && tsc --project tsconfig.server.json",
    "dev": "cross-env NODE_ENV=development API_URL=http://localhost:9000 FE_API_URL=/api nodemon",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "fake-api": "dyson .fakeAPI",
    "fake-api:watch": "nodemon --watch .fakeAPI --exec dyson .fakeAPI",
    "release": "standard-version"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "packages/*"
  ]
}
